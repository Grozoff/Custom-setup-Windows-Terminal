{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "session",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "#f1184c",
          "background": "#242424",
          "properties": {
            "display_host": false,
            "postfix": ""
          }
        },
        {
          "type": "root",
          "style": "powerline",
          "foreground": "#f1184c",
          "background": "#242424",
          "properties": {
            "postfix": "",
            "root_icon": "!"
          }
        },
        {
          "type": "time",
          "style": "powerline",
          "powerline_symbol": " ",
          "foreground": "#FFBB00",
          "background": "#242424",
          "properties": {
            "time_format": "15:04:05",
            "prefix": "",
            "postfix": ""
          }
        },
        {
          "type": "path",
          "style": "powerline",
          "powerline_symbol": " ",
          "foreground": "#33DD2D",
          "background": "#242424",
          "properties": {
            "style": "full",
            "prefix": ""
          }
        },
        {
          "type": "git",
          "style": "powerline",
          "foreground": "#3A86FF",
          "background": "#242424",
          "properties": {
            "prefix": "",
            "display_stash_count": true,
            "display_status": true,
            "display_status_detail": true,
            "display_upstream_icon": true,
            "branch_icon": "",
            "github_icon": "",
            "branch_ahead_icon": "<#88C0D0>\u21e1 </>",
            "branch_behind_icon": "<#88C0D0>\u21e3 </>",
            "local_working_icon": "<#FFAFD7>\u002a</>"
          }
        },
        {
          "type": "executiontime",
          "style": "powerline",
          "foreground": "#ffffff",
          "background": "#242424",
          "properties": {
            "threshold": 500,
            "style": "austin",
            "always_enabled": true,
            "postfix": " ",
            "prefix": ""
          }
        },
        {
          "type": "exit",
          "style": "powerline",
          "foreground": "#242424",
          "background": "#33DD2D",
          "properties": {
            "display_exit_code": false,
            "always_enabled": true,
            "color_background": true,
            "error_color": "#f1184c",
            "prefix": "‚ùØ",
            "postfix": ""
          }
        }
      ]
    }
  ],
  "final_space": true
}